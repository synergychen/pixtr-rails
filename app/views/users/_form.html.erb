<%= render "error_messages", target: user %>

<%= form_for user do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :password_digest %>
    <%= form.text_field :password_digest %>
  </div>

  <div class="submit-buttom">
    <%= form.submit %>
  </div>
<% end %>
