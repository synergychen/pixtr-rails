<%= render "error_messages", target: gallery %>

<%= form_for gallery do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>

  <div class="submit-buttom">
    <%= form.submit %>
  </div>
<% end %>
